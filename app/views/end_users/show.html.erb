<body id="mypage">


  <h1 class="mypage-title">マイページ</h1>

 <div class="mypage2">
  <div class="myname">
	<p class="mypage-name">お名前</p>
	<p class="mypage-name2"><%= @end_user.name %></p>
  </div>

  <div class="myemail">
	<p class="mypage-email">メールアドレス</p>
	<p class="mypage-email2"><%= @end_user.email %></p>
  </div>


  <div class="like-end-user">
	<p class="mypage-name">いいねした記事</p>
	<% @c.each do |e| %>
	<div class="comment_title"><%= link_to(post_path(e), class: "comment_title2", data: {"turbolinks" => false}) do %><%= Post.find(e).title %><% end %></div>
  <% end %>
  </div>


  <div class="comment-end-user">
	<p class="mypage-name">コメントした記事</p>
  <% @a.each do |c| %>
	<div class="comment_title"><%= link_to(post_path(c), class: "comment_title2", data: {"turbolinks" => false}) do %><%= Post.find(c).title %><% end %></div>
  <% end %>

  </div>
 </div>
</body>