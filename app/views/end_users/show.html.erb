<body id="mypage" style= "background-image: url(<%= image_url('sunset.jpg') %>">>


  <h1 class="mypage-title">マイページ</h1>

 <div class="mypage2">
  <div class="myname">
	<p class="mypage-name">お名前</p>
	<p class="mypage-name2"><%= @end_user.name %></p>
  </div>

  <div class="myemail">
	<p class="mypage-email">メールアドレス</p>
	<p class="mypage-email2"><%= @end_user.email %></p>
  </div>


  <div class="like-end-user">
	<p class="mypage-name">いいねした記事</p>
	<% @end_user_like.each do |end_user_likes| %>
	<div class="comment_title"><%= link_to(post_path(end_user_likes), class: "comment_title2", data: {"turbolinks" => false}) do %><%= Post.find(end_user_likes).title %><% end %></div>
  <% end %>
  </div>


  <div class="comment-end-user">
	<p class="mypage-name">コメントした記事</p>
  <% @end_user_comment.each do |end_user_comments| %>
	<div class="comment_title"><%= link_to(post_path(end_user_comments), class: "comment_title2", data: {"turbolinks" => false}) do %><%= Post.find(end_user_comments).title %><% end %></div>
  <% end %>

  </div>
 </div>
</body>
