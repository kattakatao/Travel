<div class="background-container">
  <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/1231630/moon2.png" alt="" class="Sign-up-background">
<div class="stars"></div>
<div class="twinkling"></div>
<div class="clouds"></div>

<div class="User-sign-up">
  <div class="User-sign-up2">New Member</div>
</div>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :name, class: "user-name" %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name", class: "user-name2" %>
  </div>

  <div class="field">
    <%= f.label :email, class: "user-email" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "user-email2" %>
  </div>

  <div class="field">
    <%= f.label :password, class: "user-password" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", class: "user-password2" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation, class: "user-password-agein" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "user-password-agein2" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up", class: "user-sign-up-submit" %>
  </div>
<% end %>

<%= link_to "Facebook Log-in", end_user_facebook_omniauth_authorize_path, class: "twitter-sign-up" %>
<%= link_to "Google Log-in", end_user_google_oauth2_omniauth_authorize_path, class: "google-sign-up" %>

<%#= render "end_users/shared/links" %>

