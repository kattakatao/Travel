<body id="log-in">

<!--<div class="user-log-in-main">-->
<div class="box">
  <div class="wave -one"></div>
  <div class="wave -two"></div>
  <div class="wave -three"></div>
  <div class="title">Log in</div>


<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :name, class: "Log-in-name" %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name", class: "Log-in-name2" %>
  </div>

  <div class="field">
    <%= f.label :password, class: "Log-in-password"%><br />
    <%= f.password_field :password, autocomplete: "current-password",
    class: "Log-in-password2" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me, class: "password-remember" %>
      <%= f.label :remember_me, class: "password-remember2" %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in", class: "user-log-in-submit" %>
  </div>
<% end %>

<%= link_to "Facebook Log-in", end_user_facebook_omniauth_authorize_path, class: "twitter-log-in" %>
<%= link_to "Google Log-in", end_user_google_oauth2_omniauth_authorize_path, class: "google-log-in" %>

<%= render "end_users/shared/links" %>

</div>
<!--</div>-->

</body>